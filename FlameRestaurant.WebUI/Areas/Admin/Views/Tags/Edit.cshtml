@model TagPutCommand

@{
    ViewData["Title"] = "Edit";
}

<div class="page-header">
    <div class="row align-items-end">
        <div class="col-lg-8">
            <div class="page-header-title">
                <div class="d-inline">
                    <h4>Edit this tag</h4>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="page-header-breadcrumb">
                <ul class="breadcrumb-title">
                    <li class="breadcrumb-item" style="float: left;">
                        <a asp-controller="Dashboard" asp-action="index"> <i class="feather icon-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item" style="float: left;">
                        <a asp-action="index">Tags</a>
                    </li>
                    <li class="breadcrumb-item" style="float: left;">
                        <span>Edit</span>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<div class="page-body">
    <div class="row">
        <div class="col-sm-12">
            <div class="card">
                <div class="card-block">
                    <form method="post" asp-controller="Tags" asp-action="Edit" autocomplete="off" enctype="multipart/form-data">
                        <div class="form-group row">
                            <label asp-for="Text" class="col-sm-2 col-form-label"></label>
                            <div class="col-sm-10">
                                <input asp-for="Text" class="form-control" />
                                <span asp-validation-for="Text" class="text-danger"></span>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-12 d-flex justify-content-end">
                                <button class="btn btn-primary mr-2 fix-btn">Save</button>
                                <a asp-action="index" class="btn btn-info">Cancel</a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>



